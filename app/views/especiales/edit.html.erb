<h1>Editar programa especial</h1>

<% form_for(@especial) do |f| %>
  <%= f.error_messages %>
<%= render :partial => 'form', :locals => { :f => f } %>
  
<table style="width: 900px; margin: auto; text-align: center; padding: 20px">
  <tr>
    <td>
      <%= f.submit 'Actualizar' %><% end %> |
      <%= link_to 'Ver', @especial %> |
      <%= link_to 'Atras', especiales_path %>
    </td>
  </tr>
</table>