<h1>Editar formacion</h1>

<% form_for([@docente, @formacion]) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>

<table style="width: 900px; margin: auto; text-align: center; padding: 20px">
  <tr>
    <td>
      <%= f.submit 'Actualizar' %><% end %> |
      <%= link_to 'Ver', docente_formacion_path(@formacion) %> |
      <%= link_to 'Atras', docente_formaciones_path(@docente) %>
    </td>
  </tr>
</table>