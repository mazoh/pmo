<h1>Editar proyecto</h1>

<% form_for([@docente, @linea]) do |f| %>
  <%= f.error_messages %>
<%= render :partial => 'form', :locals => { :f => f } %>
  
<table style="width: 900px; margin: auto; text-align: center; padding: 20px">
  <tr>
    <td>
      <%= f.submit 'Actualizar' %> <% end %> |
      <%= link_to 'Ver', docente_linea_path(@linea) %> |
      <%= link_to 'Atras', docente_lineas_path(@docente) %>
    </td>
  </tr>
</table>