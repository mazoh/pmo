<h1>Editar aprendiz</h1>

<% form_for([@ficha, @aprendiz]) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>

<table style="width: 900px; margin: auto; text-align: center; padding: 20px">
  <tr>
    <td>
      <%= f.submit 'Actualizar' %><% end %> |
      <%= link_to 'Ver', ficha_aprendiz_path(@ficha) %> |
      <%= link_to 'Atras', ficha_aprendices_path(@ficha) %>
    </td>
  </tr>
</table>