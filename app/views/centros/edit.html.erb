<h1>Editar centro</h1>

<% form_for(@centro) do |f| %>
  <%= f.error_messages %>
<%= render :partial => 'form', :locals => { :f => f } %>

<table style="width: 900px; margin: auto; text-align: center; padding: 20px">
  <tr>
    <td>
      <%= f.submit 'Actualizar' %><% end %> |
      <%= link_to 'Ver', @centro %> |
      <%= link_to 'Atras', centros_path %>
    </td>
  </tr>
</table>