<h1>Editar practica</h1>

<% form_for([@aprendiz, @practica]) do |f| %>
  <%= f.error_messages %>
<%= render :partial => 'form', :locals => { :f => f } %>
  
<table style="width: 900px; margin: auto; text-align: center; padding: 20px">
  <tr>
    <td>
      <%= f.submit 'Actualizar' %> <% end %> |
      <%= link_to 'Ver', aprendiz_practica_path(@practica) %> |
      <%= link_to 'Atras', aprendiz_practicas_path(@aprendiz) %>
    </td>
  </tr>
</table>