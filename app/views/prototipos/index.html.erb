<table style=" width: 900px; ">
  <tr>
    <td style="width: 380px "><h1>Inventos o Prototipos de:</h1></td>
    <td style="text-align:center; "><h2><%= @docente.nombres %></h2></td>
    <td colspan="2" style="width:230px ; text-align: right ; padding-bottom: 15px; padding-top: 15px">
     <% form_tag docente_prototipos_path, :method => 'get' do %>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Buscar", :name => nil %>
     <% end %>
    </td>
  </tr>
</table>

<div id="wrapper">
  <div id="scroll">
    <div id="scrollcontent">

<table id="tab">
  <tr>
    <th id="titul">Depositario</th>
    <th id="titul">Titulo invencion</th>
    <th id="titul">Tipo patente</th>
    <th id="titul">Pais</th>
    <th id="titul">Num registro</th>
    <th id="titul">AÃ±o</th>
    <th id="titul" colspan="3">Opciones</th>
  </tr>
  <tr id="vac"></tr>

<% @prototipos.each do |prototipo| %>
  <tr id="mayus" class="<%= cycle('odd','pair') %>">
    <td><%=h prototipo.depositario %></td>
    <td><%=h prototipo.titulo_invencion %></td>
    <td><%=h prototipo.tipo_patente %></td>
    <td><%=h prototipo.pais_id %></td>
    <td><%=h prototipo.num_registro %></td>
    <td><%=h prototipo.ano %></td>
    <td><%= link_to 'Ver', docente_prototipo_path(@docente, prototipo) %></td>
    <td><%= link_to 'Editar', edit_docente_prototipo_path(@docente, prototipo) %></td>
    <td><%= link_to 'Destruir', docente_prototipo_path(@docente, prototipo), :confirm => 'Esta Seguro?', :method => :delete %></td>
  </tr>
<% end %>
</table>
</div>
    <div id="scrollbar">
      <div id="scroller" class="scroller"></div>
    </div>
  </div>
</div>

<script type="text/javascript">
TINY.scroller.init('scroll','scrollcontent','scrollbar','scroller','buttonclick');
</script>

<table style="width: 900px; margin: auto; text-align: center; padding: 20px">
  <tr>
    <td>
      <%= link_to 'Nuevo invento o prototipo', new_docente_prototipo_path %>
      <%= link_to 'Atras', docentes_path %>
    </td>
  </tr>
</table>